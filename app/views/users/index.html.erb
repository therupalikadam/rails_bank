<p style="color: green"><%= notice %></p>

<h1>Users</h1>

<div id="users">
  <table style="border-style:solid;">
    <tr>
      <th>Email</th>
      <th>Password</th>
      <th>Token</th>
    </tr>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email%></td>
        <td><%= user.password%></td>
        <td><%= user.token%></td>
        <td><%= link_to "Show", user %></td>
        <td><%= link_to "Delete", user_path(user), method: "delete" %></td>
      </tr>
    <% end %>
  </table>
</div>

<%= link_to "New user", new_user_path %>
